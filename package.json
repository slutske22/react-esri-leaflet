{
  "name": "react-esri-leaflet",
  "version": "2.0.1",
  "description": "react components for esri-leaflet",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build && rimraf plugins",
    "checkoutput": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "test": "BABEL_ENV=development NODE_ENV=test jest --runInBand",
    "test:dev": "BABEL_ENV=development NODE_ENV=test jest --runInBand --watchAll",
    "tsc:src": "tsc --p ./src/tsconfig.json",
    "tsc:plugins": "tsc --p ./src-plugins/tsconfig.json",
    "build": "npm run clean && npm run tsc:src && npm run tsc:plugins",
    "docs:deploy": "gh-pages -d examples/ts/dist"
  },
  "files": [
    "/build",
    "/plugins",
    "/assets"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slutske22/react-esri-leaflet.git"
  },
  "keywords": [
    "react",
    "react-leaflet",
    "react-leaflet-v4",
    "esri-leaflet"
  ],
  "author": "Seth Lutske",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slutske22/react-esri-leaflet/issues"
  },
  "homepage": "https://github.com/slutske22/react-esri-leaflet#readme",
  "peerDependencies": {
    "esri-leaflet": "^3.0.14",
    "esri-leaflet-cluster": "^3.0.1",
    "esri-leaflet-geocoder": "^3.1.5",
    "esri-leaflet-heatmap": "^2.0.1",
    "esri-leaflet-vector": "^4.2.7",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/react": "^16.2.0",
    "@types/esri-leaflet": "^3.0.3",
    "@types/esri-leaflet-geocoder": "^2.2.7",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.16",
    "@types/leaflet.heat": "^0.2.4",
    "@types/leaflet.markercluster": "^1.5.5",
    "@types/react": "^19.0.10",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "esri-leaflet": "^3.0.14",
    "esri-leaflet-cluster": "^3.0.1",
    "esri-leaflet-geocoder": "^3.1.5",
    "esri-leaflet-heatmap": "^2.0.1",
    "esri-leaflet-vector": "^4.2.7",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3"
  },
  "jest": {
    "transform": {
      "\\.js$": [
        "ts-jest"
      ]
    },
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!react-leaflet)/"
    ]
  },
  "dependencies": {
    "@react-leaflet/core": "^3.0.0"
  }
}
